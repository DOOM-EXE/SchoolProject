@page
@model SchoolProjectWeb.Pages.Profesores.IndexModel
@{
    ViewData["Title"] = "Profesores";
}

<h1>Profesores</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Especialidad</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Profesores)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nombre</td>
                <td>@item.Especialidad</td>
            </tr>
        }
    </tbody>
</table>
