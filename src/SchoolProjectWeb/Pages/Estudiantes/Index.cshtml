@page
@model SchoolProjectWeb.Pages.Estudiantes.IndexModel
@{
    ViewData["Title"] = "Estudiantes";
}

<h1>Estudiantes</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Grado</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Estudiantes)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nombre</td>
                <td>@item.Apellido</td>
                <td>@item.Grado</td>
            </tr>
        }
    </tbody>
</table>
